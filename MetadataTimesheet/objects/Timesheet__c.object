    <validationRules>
        <fullName>DailyHourValidation</fullName>
        <active>true</active>
        <description>Daily hours can&apos;t exceed 24 hours.</description>
        <errorConditionFormula>Monday__c &gt; 24  ||Thursday__c &gt; 24 || Tuesday__c &gt; 24 ||Wednesday__c &gt; 24 ||Friday__c &gt; 24</errorConditionFormula>
        <errorMessage>Daily hours can&apos;t exceed 24 hours</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Validation</fullName>
        <active>true</active>
        <description>End Date must be the same as Start Date or after.</description>
        <errorConditionFormula>End_Date__c  &lt;  Start_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End Date must be the same as Start Date or after.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hour_rate_validation</fullName>
        <active>true</active>
        <description>Hour rate can&apos;t be over $100</description>
        <errorConditionFormula>Hour_Rate__c  &gt;100</errorConditionFormula>
        <errorDisplayField>Hour_Rate__c</errorDisplayField>
        <errorMessage>Hour rate can&apos;t be over $100</errorMessage>
    </validationRules>
